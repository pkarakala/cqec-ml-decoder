    "## Part 9: Accuracy Over Time (Drift Analysis)\n",
    "\n",
    "Analyze how decoder performance changes as parameters drift.\n",
    "Split test set into early, middle, late portions."
   ],
   "id": "part9-header"
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Split test trajectories into temporal segments\n",
    "test_trajectories = data_p4['dataset'][data_p4['params']['n_train']:]\n",
    "\n",
    "# For each trajectory, split into early/middle/late windows\n",
    "def split_trajectory_windows(traj, window_size=20, n_segments=5):\n",
    "    \"\"\"Split trajectory into temporal segments and create windows.\"\"\"\n",
    "    T = len(traj['r1'])\n",
    "    segment_size = T // n_segments\n",
    "    \n",
    "    segments = []\n",
    "    for seg_idx in range(n_segments):\n",
    "        start = seg_idx * segment_size\n",
    "        end = start + segment_size if seg_idx < n_segments - 1 else T\n",
    "        \n",
    "        # Create windows for this segment\n",
    "        X_seg, y_seg = [], []\n",
    "        for t in range(start + window_size, end):\n",
    "            window = np.stack([\n",
    "                traj['r1'][t - window_size:t],\n",
    "                traj['r2'][t - window_size:t]\n",
    "            ], axis=1)\n",
    "            X_seg.append(window)\n",
    "            y_seg.append(traj['error_state'][t - 1])\n",
    "        \n",
    "        if len(X_seg) > 0:\n",
    "            segments.append({\n",
    "                'X': np.array(X_seg),\n",
    "                'y': np.array(y_seg),\n",
    "                'time_range': (start, end)\n",
    "            })\n",
    "    \n",
    "    return segments\n",
    "\n",
    "print(\"Analyzing accuracy over time...\")\n",
    "print(f\"Splitting {len(test_trajectories)} test trajectories into 5 temporal segments\")\n",
    "\n",
    "# Collect windows from each segment across all test trajectories\n",
    "n_segments = 5\n",
    "segment_data = [{'X': [], 'y': []} for _ in range(n_segments)]\n",
    "\n",
    "for traj in test_trajectories:\n",
    "    segments = split_trajectory_windows(traj, window_size=WINDOW_SIZE, n_segments=n_segments)\n",
    "    for seg_idx, seg in enumerate(segments):\n",
    "        segment_data[seg_idx]['X'].append(seg['X'])\n",
    "        segment_data[seg_idx]['y'].append(seg['y'])\n",
    "\n",
    "# Concatenate\n",
    "for seg_idx in range(n_segments):\n",
    "    segment_data[seg_idx]['X'] = np.concatenate(segment_data[seg_idx]['X'])\n",
    "    segment_data[seg_idx]['y'] = np.concatenate(segment_data[seg_idx]['y'])\n",
    "    print(f\"  Segment {seg_idx+1}: {len(segment_data[seg_idx]['X']):,} windows\")"
   ],
   "id": "split-temporal"
  }
 ]
}
